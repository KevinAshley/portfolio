{
    "name": "portfolio",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "docker:build_dev": "docker build -f Dockerfile.dev -t dev_image . ",
        "docker:run_dev": "docker run -dp 3000:3000 -w /app --mount type=bind,src='./',target=/app dev_image",
        "docker:dev": "npm run docker:build_dev && npm run docker:run_dev",
        "docker:build": "docker build -t production_image .",
        "docker:run": "docker run -dp 3000:3000 --mount type=bind,src='./prisma/database',target=/app/prisma/database production_image",
        "prisma:create_db": "npx prisma migrate dev --name create_db",
        "prisma:dangerously_unlock_dev_db_permissions": "chmod -R 777 prisma/database",
        "prisma:create_dev_db": "npm run prisma:create_db && npm run prisma:dangerously_unlock_dev_db_permissions",
        "prisma:create_migration_from_schema": "npx prisma migrate dev",
        "prisma:update_prisma_client": "npx prisma generate",
        "prisma:migrate": "npm run prisma:create_migration_from_schema && npm run prisma:update_prisma_client",
        "prisma:dev_database_seed": "NODE_ENV=development npx prisma db seed",
        "prisma:prod_database_seed": "NODE_ENV=production npx prisma db seed"
    },
    "scriptsComments": {
        "docker_build_args: -t": "Assigns a name to the image",
        "docker_build_args: -f": "Specify a dockerfile",
        "docker_run_args: -d": "Detach from console when done",
        "docker_run_args: -p": "Publish container port to host",
        "docker_run_args: -w": "Working directory inside the container",
        "docker_run_args: --mount": "Attach filesystem mount to the container (w/ type,src,target)"
    },
    "dependencies": {
        "@emotion/cache": "^11.13.0",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4",
        "@mui/material-nextjs": "^5.16.4",
        "@mui/system": "^5.16.4",
        "@prisma/client": "^5.18.0",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "next": "14.2.5",
        "next-auth": "^5.0.0-beta.20",
        "react": "^18",
        "react-dom": "^18",
        "three": "^0.168.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/three": "^0.168.0",
        "eslint": "^8",
        "eslint-config-next": "14.2.5",
        "prisma": "^5.18.0",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    }
}
